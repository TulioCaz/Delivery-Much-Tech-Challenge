{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "prettier"

    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "camelcase": "off",
      "no-plusplus": "off",
      "no-await-in-loop": "off"
    }
}
